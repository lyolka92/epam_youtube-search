{
  "env": {
	"browser": true,
	"es2020": true
  },
  "extends": [
	"airbnb-base"
  ],
  "parserOptions": {
	"ecmaVersion": 11,
	"sourceType": "module"
  },
  "rules": {
	"semi": [
	  "error",
	  "always"
	],
	"quotes": [
	  "error",
	  "single"
	],
	"indent": [
	  "error",
	  "tab"
	],
	"no-tabs": "off",
	"func-names": [
	  "warn",
	  "as-needed"
	],
	"max-len": [
	  "warn",
	  {
		"code": 130
	  }
	],
	"import/prefer-default-export": "off"
  }
}
